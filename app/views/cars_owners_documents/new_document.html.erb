<h1>Add Document</h1>

<%= form_with(url: add_document_path(user_id: @user.id), method: :put) do |form| %>
  <%= form.collection_check_boxes 'cars_owners_documents[documents]', @cars_owners_documents, :id, :name%>
  <br>

  <%= form.submit 'Add'  %>
<% end %>

<%= link_to 'Back', cars_owners_documents_path(user_id: @user.id) %>
