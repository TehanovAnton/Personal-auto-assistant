<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Year production</th>
      <th>Engine volume</th>
      <th>Mileage</th>
      <th>Body type</th>
      <th>Fuel type</th>
      <th>Transmission type</th>
      <th>Maker</th>
      <th>Vin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.model %></td>
        <td><%= car.year_production %></td>
        <td><%= car.engine_volume %></td>
        <td><%= car.mileage %></td>
        <td><%= car.body_type %></td>
        <td><%= car.fuel_type %></td>
        <td><%= car.transmission_type %></td>
        <td><%= car.maker %></td>
        <td><%= car.vin %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Consumables', car_consumable_values_path(car_id: car.id) %></td>
        <td><%= link_to 'Parts', parts_path(car) %></td>
        <td><%= link_to 'Orders', orders_path(car_id: car.id) %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
