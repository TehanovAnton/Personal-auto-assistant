<p id="notice"><%= notice %></p>

<h1>Car Consumable Values</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_consumable_values.each do |car_consumable_value| %>
      <tr>
        <td><%= car_consumable_value.value %></td>
        <td><%= link_to 'Show', car_consumable_value %></td>
        <td><%= link_to 'Edit', edit_car_consumable_value_path(car_consumable_value) %></td>
        <td><%= link_to 'Destroy', car_consumable_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Consumable Value', new_car_consumable_value_path %>
