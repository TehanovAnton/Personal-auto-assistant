<h1>Add Organization Service</h1>

<%= form_with(url: organization_service_works_path(@organization), method: :get) do |form| %>

  <%= collection_check_boxes 'organization', 'services', @services, :id, :name %>
  <br>

  <%= form.submit 'Next'  %>
<% end %>

<%= link_to 'Back', organization_services_path(@organization) %>
